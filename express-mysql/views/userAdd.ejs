        <div>
            <label>聯絡內容：</label>
            <input type="text" name="contact" placeholder="聯絡內容">
        </div>
        <div>
            <label>備註：</label>
            <input type="text" name="remark" placeholder="備註">
        </div>
        <div>
            <input type='submit' value='設定'>
            <input type='reset' value='清除'>
            <input type='button' value='回前頁' onclick="javascript: location.href='/'">
        </div>
    </form>
</body>

</html>